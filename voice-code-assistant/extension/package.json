{
  "name": "voice-code-assistant",
  "displayName": "Voice Code Assistant",
  "publisher": "yourname",
  "version": "0.0.1",
  "engines": { "vscode": "^1.60.0" },
  "activationEvents": [ "onCommand:voiceAssist.start" ],
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p .",
    "watch": "tsc -w"
  },
  "contributes": {
    "commands": [
      {
        "command": "voiceAssist.start",
        "title": "Start Voice Assistant"
      }
    ]
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "typescript": "^4.0.0",
    "@types/node": "^14.0.0",
    "@types/vscode": "^1.60.0",
    "vscode": "^1.1.37"
  }
}
